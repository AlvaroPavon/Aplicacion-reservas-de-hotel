<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.reservas.GestionClientesController" spacing="10" alignment="CENTER">
    <TableView fx:id="table">
        <columns>
            <TableColumn fx:id="colNombre" text="Nombre"/>
            <TableColumn fx:id="colApellido" text="Apellido"/>
            <TableColumn fx:id="colEmail" text="Email"/>
        </columns>
    </TableView>
    <Button text="Agregar Cliente" fx:id="agregarCliente" onAction="#agregarCliente"/>
    <Button text="Eliminar Cliente" fx:id="eliminarCliente" onAction="#eliminarCliente"/>
    <Button text="Editar Cliente" fx:id="editarCliente" onAction="#editarCliente"/>
</VBox>

